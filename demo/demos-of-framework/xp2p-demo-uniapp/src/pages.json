{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "xp2p demo"
			}
		}
	],
	"plugins": {
		"xp2p": {
			"version": "latest",
			"provider": "wx1319af22356934bf",
			"export": "exportForXp2pPlugin.js"
		}
	},
	"subPackages": [
		{
			"root": "pages/video",
			"pages": [
				{
					"path": "ipc-demo",
					"style": {
						"usingComponents": {
							"iot-p2p-player-with-mjpg": "plugin://xp2p/iot-p2p-player-with-mjpg",
							"iot-p2p-intercom": "plugin://xp2p/iot-p2p-intercom"
						}
					}
				}
			],
			"plugins": {
				"wechat-p2p-player": {
					"version": "latest",
					"provider": "wx9e8fbc98ceac2628",
					"export": "exportForPlayerPlugin.js"
				},
				"wmpf-voip": {
					"version": "latest",
					"provider": "wxf830863afde621eb"
				}
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			"^demo-(.*)": "@components/demo-$1.vue"
		}
	}
}